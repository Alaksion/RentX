<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rentx">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RentX">
        <activity
            android:name=".teste.tab_act"
            android:label="@string/title_activity_tab_act"
            android:theme="@style/Theme.RentX.NoActionBar"></activity>
        <activity android:name=".presentation.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.home.HomeActivity"
            android:label="@string/title_activity_home" />
        <activity android:name=".presentation.authentication.AuthenticationActivity" />
        <activity android:name=".presentation.onboarding.OnboardingActivity" />
    </application>

</manifest>